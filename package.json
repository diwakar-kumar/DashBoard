{
  "name": "dashBoard",
  "version": "1.0.0",
  "description": "Simple Customize Dashboard",
  "main": "index.js",
  "scripts": {
    "test": "`npm bin`/mocha --compilers js:babel-core/register --require ./app/tests/test_helper.js '*/tests/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "webserver": "node api/src/server.js",
    "build": "`npm bin`/webpack",
    "dev": "`npm bin`/webpack-dev-server",
    "lint": "`npm bin`/eslint . --ext .js --ext .jsx --fix",
    "deploy": "NODE_ENV=production `npm bin`/webpack -p",
    "start": "npm run dev",
    "clean": "rm -rf app/dist app/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dheeraj-Karki/DashBoard.git"
  },
  "keywords": [
    "koa",
    "redux",
    "react",
    "webpack",
    "mocha",
    "sass"
  ],
  "author": "Dheeraj Karki",
  "license": "ISC",
  "homepage": "https://github.com/Dheeraj-Karki/DashBoard.git",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bookshelf": "^0.10.3",
    "css-loader": "^0.28.4",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.13.0",
    "koa": "^2.3.0",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.3",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.0.2",
    "chai-immutable": "^1.6.0",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "npm-install-webpack-plugin": "^4.0.4",
    "react-css-modules": "^4.3.0",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.4.0",
    "webpack-dev-server": "^2.5.0"
  }
}
